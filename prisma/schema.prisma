generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MataKuliah {
  id           Int      @id @default(autoincrement())
  nama         String
  kodeUnik     String   @unique
  namaDosen    String
  linkUrlImage String
}

model Mahasiswa {
  id   Int    @id @default(autoincrement())
  nama String
  nim  String @unique
}